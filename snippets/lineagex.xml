<?xml version="1.0" encoding="UTF-8"?>
<manifest>
    
    <remote name="lineagex"
            fetch="https://github.com//Lineage-X" />
    
    <remote name="teamorchidos"
            fetch="https://github.com/teamorchidos" />
    
    <project path="vendor/oplauncher" name="android_vendor_oplauncher" remote="teamorchidos" revision="Orchidos" />
    <project path="vendor/prebuilts" name="android_vendor_prebuilts" remote="teamorchidos" revision="Orchidos" />
    <project path="vendor/OneplusCamera" name="vendor_OneplusCamera" remote="teamorchidos" revision="ten" />
    <project path="vendor/lawnchair" name="android_vendor_lawnchair-1" remote="teamorchidos" revision="android_10" />
    <project path="vendor/partner_gms" name="vendor_partner_gms" remote="teamorchidos" revision="master" />
    <project path="packages/apps/Settings" name="android_packages_apps_Settings-1" remote="teamorchidos" revision="ten" />
    <project path="android" name="android" remote="lineagex" />
    <project path="vendor/lineage" name="android_vendor_lineage" remote="lineagex" />
    <project path="frameworks/base" name="android_frameworks_base" remote="lineagex" />
    <project path="lineage-sdk" name="android_lineage-sdk" remote="lineagex" />
    <project path="device/lineage/sepolicy" name="android_device_lineage_sepolicy" remote="lineagex" />
    <project path="packages/apps/Updater" name="android_packages_apps_Updater" remote="lineagex" />
    <project path="packages/services/Telecomm" name="android_packages_services_Telecomm" remote="lineagex" />
    <project path="system/core" name="android_system_core" remote="lineagex" />
    <project path="packages/apps/LineageParts" name="android_packages_apps_LineageParts" remote="lineagex"/>
    <project path="build/soong" name="android_build_soong" remote="lineagex" >
        <linkfile src="root.bp" dest="Android.bp" />
        <linkfile src="bootstrap.bash" dest="bootstrap.bash" />
    </project>
    <project path="art" name="android_art" remote="lineagex" />
    <project path="build/make" name="android_build" remote="lineagex" >
      <copyfile src="core/root.mk" dest="Makefile" />
      <linkfile src="CleanSpec.mk" dest="build/CleanSpec.mk" />
      <linkfile src="buildspec.mk.default" dest="build/buildspec.mk.default" />
      <linkfile src="core" dest="build/core" />
      <linkfile src="envsetup.sh" dest="build/envsetup.sh" />
      <linkfile src="target" dest="build/target" />
      <linkfile src="tools" dest="build/tools" />
    </project>

    <!-- External -->
    <project path="external/asus/StitchImage" name="external_asus_StitchImage" remote="lineagex" />
    <project path="external/motorola/faceunlock" name="external_motorola_faceunlock" remote="lineagex" />
</manifest>
